{
    "name": "rbac-mysql-backend",
    "version": "1.0.0",
    "description": "Role-Based Access Control System with MySQL and Sequelize",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "db:create": "npx sequelize-cli db:create",
        "db:drop": "npx sequelize-cli db:drop",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed"
    },
    "keywords": [
        "rbac",
        "mysql",
        "sequelize",
        "express",
        "nodejs",
        "jwt",
        "authentication"
    ],
    "author": "Prince",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.11.0",
        "sequelize": "^6.37.3"
    },
    "devDependencies": {
        "nodemon": "^3.1.4",
        "sequelize-cli": "^6.6.2"
    }
}
